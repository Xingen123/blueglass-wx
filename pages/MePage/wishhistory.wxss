/* pages/MePage/wishhistory.wxss */

.wish_history_contianer {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.wish_history_cell {
  margin-left: 26rpx;
  width: calc(100% - 52rpx);
  background: #fff;
  box-shadow: 0 4px 40px 0 rgba(215, 215, 215, 0.50);
  border-radius: 12rpx;
  height: 380rpx;
  align-items: center;
}

.top_title {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #01a0eb;
  margin-left: 22rpx;
}

.top_title_gray {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #9b9b9b;
  margin-left: 22rpx;
}

.cell_top_view {
  margin-top: 25rpx;
  width: 127rpx;
  height: 56rpx;
  border: 2px solid #01a0eb;
  border-radius: 0px 40.5px 40.5px 0px;
}

.cell_top_view_gray {
  margin-top: 25rpx;
  width: 127rpx;
  height: 56rpx;
  border: 2px solid #9b9b9b;
  border-radius: 0px 40.5px 40.5px 0px;
}

.cell_view {
  height: 70rpx;
  width: 100%;
  align-items: center;
  display: flex;
}

.cell_title {
  margin-left: 40rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #9b9b9b;
}

.cell_info {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #000;
  margin-left: 40rpx;
}

.miview {
  width: 100%;
  height: 30rpx;
}
