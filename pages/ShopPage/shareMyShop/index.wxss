.bacdground_imgurl {
  height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.sologn_con {
  margin-top:60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.sologn_con image {
  height: 38rpx;
  width: 38rpx;
}

.sologn {
  text-align: center;
  font-size: 12px;
  color: #fff;
  border-radius: 50rpx;
  height: 70rpx;
  background: rgba(0, 0, 0, 0.4);
  padding: 0 10rpx;
  line-height: 70rpx;
  width: 90%;
}

.shop {
  /* display: flex;
  align-items: center;
  justify-content: flex-start; */
  position: relative;
  background: rgba(0, 0, 0, 0.4);
  border-radius: 50rpx 10rpx 10rpx 50rpx;
  margin-top: 40rpx;
  width: auto;
  padding: 0 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-items: center;
}

.shop_right {
  display: flex;
  margin-top: 10rpx;
  /* margin-left: -50rpx; *//* z-index: -1; */
}

.shop image {
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
  position: absolute;
  left: 0;
  top: -6rpx;
}

.shop_name {
  font-size: 12px;
  color: #fff;
  margin-left: 10px;
}

.shop_dj {
  font-size: 12px;
  color: #fff;
  margin-left: 20px;
}

.shop_sale {
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50rpx;
}

.shop_sale_ymc {
  font-size: 12px;
  /* color: #9b9b9b; */
  color: #fff;
}

.shop_sale_ymc span {
  font-size: 14px;
  /* color: #01a0eb; */
  color: #fff;
  line-height: 33px;
}

.shop_sale_jl {
  font-size: 12px;
  /* color: #01a0eb; */
  color: #fff;
  border: 1px solid #fff;
  border-radius: 10px;
  padding: 0px 16px;
  margin-left: 20px;
  margin-top: 0px;
}

.circle {
  /* background-image: linear-gradient(-270deg, #02cef7 0%, #01a0eb 100%); *//* box-shadow: 0 4px 33px 0 #c0ebff; *//* position: fixed; *//* bottom: 100rpx; *//* font-size: 13px; *//* color: #fff; *//* border-radius: 50%; *//* text-align: center; */
}

.zindex {
  z-index: 999;
}

.yqhy image {
  position: fixed;
  left: 20rpx;
  height: 252rpx;
  width: 206rpx;
  bottom: 0rpx;
}

.diy image {
  position: fixed;
  right: 20px;
  height: 282rpx;
  width: 230rpx;
  bottom: 0rpx;
}

.fx image {
  position: fixed;
  left: 100rpx;
  height: 252rpx;
  width: 252rpx;
  bottom: 0rpx;
}

.mark_dialog {
  background: rgba(0, 0, 0, 0.4);
  height: 100%;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 990;
}

.showdialog {
  padding: 10rpx 20rpx 20rpx 20rpx;
  font-size: 26rpx;
  color: #fff;
  border: 2px dashed #fff;
  border-radius: 6px;
  position: fixed;
  left: 150rpx;
  bottom: 200rpx;
  width: 400rpx;
  height: 150rpx;
  line-height: 0.7;
  z-index: 999;
}

.mark_second {
  left: 250rpx;
  line-height: 30rpx;
  width: 450rpx;
}
.mark_third{
  left:89rpx;
  bottom:212rpx;
  width: 450rpx;
}
.show {
  display: block;
}

.hide {
  display: none;
}
.back2View
{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.back2View image
{
  width: 100%;
  height: 100%;
}
.tipsView
{
    position: fixed;
    left: 100rpx;
    right: 100rpx;
  height: 85rpx;
  bottom: 60rpx;
  background-color: #D8D8D8;
  text-align: center;
  line-height: 28rpx;
}
.tipsView text
{
  font-family: PingFangSC-Medium;
font-size: 28rpx;
color: #4A4A4A;
