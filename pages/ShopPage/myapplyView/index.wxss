.back2View image {
  width: 100%;
  height: calc(100% - 100rpx);
}

.tipsView {
  position: fixed;
  left: 100rpx;
  right: 100rpx;
  height: 85rpx;
  bottom: 160rpx;
  background-color: #fff;
  text-align: center;
  line-height: 28rpx;
  display: flex;
  align-content: space-between;
}

.tipsView text {
  margin-left: 30rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #4a4a4a;
}

.tipsView image {
  margin-left: 20rpx;
  margin-top: 23rpx;
  width: 40rpx;
  height: 40rpx;
}

.back2View {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.kaidianView {
  background: #fff;
  box-shadow: 0 -7rpx 71rpx 0 rgba(234, 234, 234, 0.50);
  width: 100%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  bottom: 0rpx;
  position: fixed;
}

.textkaidian {
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #01a0eb;
}

.titleView {
  background: rgba(0, 0, 0, 0.30);
  box-shadow: 0 -7rpx 71rpx 0 rgba(209, 209, 209, 0.50);
  border-radius: 6rpx;
  width: 270rpx;
  height: 82rpx;
  text-align: center;
  line-height: 80rpx;
  top: 55rpx;
  right: 30rpx;
  position: fixed;
}

.titleView text {
  font-family: PingFangSC-Medium;
  font-size: 50rpx;
  color: #fff;
}

.infoView {
  background: rgba(0, 0, 0, 0.30);
  box-shadow: 0 -7rpx 71rpx 0 rgba(209, 209, 209, 0.50);
  border-radius: 6rpx;
  width: 403rpx;
  height: 124rpx;
  text-align: right;
  top: 150rpx;
  right: 30rpx;
  position: fixed;
  padding-top:10rpx;

}

.infoView text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #fff;
  text-align: right;
}

